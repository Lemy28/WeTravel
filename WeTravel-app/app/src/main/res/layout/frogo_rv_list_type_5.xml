<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frogo_rv_list_type_5_root_relative_layout"
    style="@style/FrogoLayoutList8dp">

    <ImageView
        android:id="@+id/iv_wish_image"
        style="@style/frogoArea48dp"
        android:layout_centerVertical="true"
        android:contentDescription="@string/frogo_dummy_content_description"
        tools:src="@tools:sample/avatars" />

    <LinearLayout
        android:id="@+id/frogo_rv_list_type_5_container_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="@dimen/frogo_dimen_16dp"
        android:layout_toEndOf="@id/iv_wish_image"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_wish_name"
            style="@style/frogoTvListTitle"
            tools:text="@tools:sample/lorem/random" />

        <TextView
            android:id="@+id/tv_wish_address"
            style="@style/FrogoTvListSubTitle"
            tools:text="@tools:sample/lorem/random" />

    </LinearLayout>

</RelativeLayout>